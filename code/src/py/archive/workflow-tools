digraph {
	fetch_record [label="Fetch Record"]
	fetch_history [label="Fetch History"]
	detect_anomaly [label="Detect Anomaly"]
	decide_action [label="Decide Action"]
	execute_action [label="Execute Action"]
	fetch_record -> fetch_history
	fetch_history -> detect_anomaly
	detect_anomaly -> decide_action
	decide_action -> execute_action
}
